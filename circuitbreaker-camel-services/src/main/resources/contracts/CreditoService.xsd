<?xml version="1.0" encoding="UTF-8"?>
<schema
	xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://soaexpert.com.br/CreditoService/"
	xmlns:tns="http://soaexpert.com.br/CreditoService/"
	xmlns:dom="http://soaexpert.com.br/domain"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified">
	<xsd:import
		schemaLocation="../schemas/DomainModel.xsd"
		namespace="http://soaexpert.com.br/domain" />
	<xsd:element name="verificaCreditoSerasa">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element
					name="empresa"
					type="dom:Empresa" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="verificaCreditoSerasaResponse">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element
					name="credito"
					type="dom:Credito" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="verificaCreditoSerasaFault">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element
					name="verificaCreditoFault"
